function [ind] = getLineIndeces(p1,p2)

    m = (p2(1)-p1(1))/(p2(2)-p1(2));
    b = p2(1) - ( m * p2(2) );

    count = 0;
    ind = [];
    minR = min(p1(1), p2(1));
    maxR = max(p1(1), p2(1));
    for r=minR:maxR
        count = count + 1;
        ind(count, 1) = r;
        ind( count, 2) = round( (r - b) / m);
    end
